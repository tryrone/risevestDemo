{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "types": ["@types/styled-components-react-native"],
    "baseUrl": ".",
    "paths": {
      "*": ["src/*"],
      "test/*": ["test/*"],
      "components/*": ["src/components/*"],
      "pages": ["src/pages/*"],
      "navigation": ["src/navigation/*"],
      "utils": ["src/utils/*"],
      "constants": ["src/constants/*"],
      "rtk": ["src/rtk/*"],
      "assets": ["src/assets/*"],
      "schemas": ["src/schemas/*"]
    }
  }
}
